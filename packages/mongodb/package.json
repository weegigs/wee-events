{
  "name": "@weegigs/events-mongo",
  "version": "0.13.14",
  "description": "Mongo event store and projections for @weegigs/events",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Kevin O'Neill <kevin@oneill.id.au>",
  "scripts": {
    "build": "yarn run clean && yarn run lint && yarn run compile",
    "clean": "rimraf lib",
    "compile:tsc": "tsc -p .",
    "compile": "yarn run compile:tsc",
    "lint": "tslint -c tslint.json --project . \"src/**/*.ts\"",
    "test:unit": "npx jest \"tests/*\" --env=node --coverage --detectOpenHandles",
    "test": "yarn run test:unit",
    "watch:tsc": "tsc -p . --watch",
    "watch:test": "npx jest \"tests/*\" --env=node --coverage --watch",
    "watch": "yarn run watch:tsc & yarn run watch:test",
    "prepublishOnly": "yarn run clean && yarn run lint && yarn run compile"
  },
  "devDependencies": {
    "@types/dotenv": "6.1.0",
    "@types/faker": "4.1.5",
    "@types/jest": "24.0.11",
    "@types/lodash": "4.14.123",
    "@types/mongodb": "3.1.22",
    "@types/mz": "0.0.32",
    "@types/node": "11.11.4",
    "@types/ramda": "0.26.3",
    "@weegigs/events-core": "0.13.14",
    "concurrently": "4.1.0",
    "dotenv": "7.0.0",
    "faker": "4.1.0",
    "jest": "24.5.0",
    "jest-cli": "24.5.0",
    "jest-environment-node-debug": "2.0.0",
    "prettier": "1.16.4",
    "ramda": "0.26.1",
    "rimraf": "2.6.3",
    "rxjs": "6.4.0",
    "rxjs-tslint": "0.1.7",
    "ts-jest": "24.0.0",
    "tslint": "5.14.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.3.4000",
    "winston": "3.2.1"
  },
  "dependencies": {
    "@weegigs/concurrent": "1.0.1",
    "lodash": "4.17.21",
    "mongodb": "3.1.13"
  },
  "peerDependencies": {
    "@weegigs/events-core": "^0.13",
    "rxjs": "^6.4",
    "winston": "^3.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "mapCoverage": true
  },
  "engines": {
    "node": ">=8.9"
  },
  "files": [
    "lib",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENCE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinoneill/wee-events.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
